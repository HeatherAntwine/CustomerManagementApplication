        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <ButtonBar layoutX="265.0" layoutY="281.0" prefHeight="40.0" prefWidth="189.0">
        <buttons>
          <Button fx:id="addButt" mnemonicParsing="false" onAction="#addButtAction" text="Add" />
            <Button fx:id="modButt" mnemonicParsing="false" onAction="#modButtAction" text="Modify" />
            <Button fx:id="delButt" mnemonicParsing="false" onAction="#delButtAction" text="Remove" />
        </buttons>
      </ButtonBar>
      <Button fx:id="backButt" layoutX="730.0" layoutY="342.0" mnemonicParsing="false" onAction="#backButtAction" prefHeight="25.0" prefWidth="64.0" text="Back" />
